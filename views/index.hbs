<body>
  <!-- <div id="loader"></div> -->
  <!-- <div class="container"> -->
  <div class="col-lg-3 col-md-3 col-sm-12 nopadding">

    <!-- LOGO -->
    <div class="logo nopadding">
      <img style="max-width:10em;" src="/img/treffen-logo.png" alt="treffen logo the most socialist meetup app">
      <i class="fa fa-2x fa-bars" aria-hidden="true"></i>
      <i class="fa fa-1x fa-ellipsis-v" aria-hidden="true"></i>
    </div>

    <!-- LEFTBAR -->
    <div class="leftbar">
      <h2><img style="max-width: 1em;" src="/img/treffen-logo1.png" alt="treffen meetup app">TRY</h2>
      <p>Type in zip codes separated by commas to find your place to meet.</p>
      <br>

      <form action="/zip" method="post">
        <div class="row">
          <div class="col-lg-2 col-md-2 col-sm-2 alignright">
            <i class="fa fa-hashtag" aria-hidden="true"></i>
          </div>
          <div class="col-lg-10 col-md-10 col-sm-10">
            <div class="input-group">
              <span class="input-group-addon" id="sizing-addon1">></span>
              <input type="text" class="form-control" name="zip" placeholder="Zip Codes" aria-label="Username" aria-describedby="sizing-addon1">
            </div>
          </div>
        </div>
        <br>
        <div class="row">
          <div class="col-lg-2 col-md-2 col-lg-2 alignright">
            <i class="fa fa-yelp" aria-hidden="true"></i>
          </div>
          <div class="col-lg-10 col-md-10 col-lg-10">
            <select class="form-control" name="term" id="select-cat" placeholder="Select a category">
              <option selected >Select a category</option>
              <option value="coffee">Coffee</option>
              <option value="tea">Tea</option>
              <option value="bars">Bars</option>
              <option value="animalshelter">Animal Shelters</option>
              <option value="airport">Airport</option>
              <option value="aquarium">Aquariums</option>
              <option value="pizza">Pizza</option>
              <option value="museum">Museums</option>
              <option value="zoos">Zoos</option>
              <option value="japanese">Japanese</option>
              <option value="noodles">Noodles</option>
              <option value="dessert">Dessert</option>
              <option value="bubbletea">Bubble Tea</option>
              <option value="restaurant">Restaurants</option>
              <option value="indian">Indian</option>
            </select>
            <a href="#"></a>
          </div>
        </div>
        <br><br>
        <input type="submit" name="button" class "" value="GO">
      </form>
    </div>

<div class="matrix">
    <div class="row">
      <div class="col-lg-2 col-md-2 col-lg-2 alignright">
        <i class="fa fa-clock-o" aria-hidden="true"></i>
      </div>
      <div class="col-lg-10 col-md-10 col-lg-10">
        <p>Travel Time: {{matrix}}</p>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-2 col-md-2 col-lg-2 alignright">
        <i class="fa fa-road" aria-hidden="true"></i>
      </div>
      <div class="col-lg-10 col-md-10 col-lg-10">
        <p>Distance: </p>
      </div>
    </div>
  </div>

    <div class="contact nopadding">
      <hr class="primary">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 nopadding" style="text-align: center;">
          <a href="https://www.linkedin.com/in/julianamei/" class="btn btn-default" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a>
          <a href="https://github.com/julianapeace" class="btn btn-default" target="_blank"><i class="fa fa-github fa-2x"></i></a>
          <a href="https://drive.google.com/file/d/1SO_jvjZAtjL8XHF1w7Dm91ISb4foVuk9/view?usp=sharing" class="btn btn-default" target="_blank"><i class="fa fa-file-text-o fa-2x"></i></a>
        </div>
      </div>
      <br>
      <div class="row" style="text-align: center;">
        <div class="col-md-6 nopadding">
          <i class="fa fa-phone fa-2x"></i>
        </div>
        <div class="col-md-6 nopadding">
          <i class="fa fa-envelope-o fa-2x"></i>
          <p><a href="mailto:chancecordelia@gmail.com"></a></p>
        </div>
      </div>
    </div>

  </div>
  <!-- MAP -->
  <div class="col-lg-9 col-md-9 col-sm-12 nopadding">
    <div id="map"></div>
  </div>
